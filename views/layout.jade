    meta(charset="UTF-8")
    meta(name="viewport" content="width=device-width,initial-scale=1")
    meta(http-equiv="X-UA-Compatible" content="IE=edge")
    link(href="http://7xq9bs.com1.z0.glb.clouddn.com/albin-ga/imgs/favico.jpg" rel="icon")
    link(rel='stylesheet', media="screen" href='http://7xq9bs.com1.z0.glb.clouddn.com/albin-ga/stylesheets/font-awesome.css')
    link(rel='stylesheet', href='http://7xq9bs.com1.z0.glb.clouddn.com/albin-ga/stylesheets/prettify.css')
    script(src="http://7xq9bs.com1.z0.glb.clouddn.com/albin-ga/javascripts/prettify.js")
